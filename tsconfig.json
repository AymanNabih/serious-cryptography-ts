{
    "compilerOptions": {
        "sourceMap": true,
        "strictFunctionTypes": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "suppressImplicitAnyIndexErrors": true,
        "module": "commonjs",
        "target": "es6",
        "baseUrl": "src",
        "outDir": "./dist"
    },
    "exclude": [
        "dist",
        "node_modules"
    ]
}